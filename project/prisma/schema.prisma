generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Jackets {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image1 String? @default("")
  image2 String? @default("")
  image3 String? @default("")
  image4 String? @default("")
}

