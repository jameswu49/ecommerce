generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Jackets {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}

model Shirts {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}

model Tops {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}

model Sweatshirts {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}

model Pants {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}

model Accessories {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}

model Shoes {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  price Float 
  image_front String
  image_back String
}
